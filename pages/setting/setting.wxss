/* pages/setting/setting.wxss */
form{
    display: block;
    width: 100%;
    overflow: hidden;
}
.inpText{
    border: 1rpx solid #ccc;
    margin: 20rpx 20rpx 0;
    padding-left: 20rpx;
}
.placeholder{
    font-size: 24rpx;
}
.btn{
    width: 400rpx;
    height: 100rpx;
    margin-top: 20rpx;
    font-size: 30rpx;
    line-height: 100rpx;
}
.box{
    width: 200rpx;
    height: 200rpx;
    margin: 20rpx auto 0;
    background: #868686;
}
.item-wrap{
    padding: 10rpx;
    display: flex;
}
.img-wrap{
    width: 160rpx;
    height: 160rpx;
}
image{
    width: 100%;
    height: 100%;
}
.item-content{
    flex: 1;
    margin-left: 10rpx;
    border-bottom: 1px solid #ccc;
}